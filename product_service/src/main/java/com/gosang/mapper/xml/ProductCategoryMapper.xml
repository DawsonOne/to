<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gosang.mapper.ProductCategoryMapper">
<select id="findCategoryName" parameterType="java.lang.Integer" resultType="java.lang.String">
    select category_name from product_category where category_type = #{categoryId}
</select>
</mapper>
